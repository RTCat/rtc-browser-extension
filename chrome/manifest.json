{
  "name": "Linctime ScreenSharing Extension",
  "description": "Linctime ScreenSharing Extension 0.5",
  "version": "0.5.2",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon32.png",
    "default_title": "Linctime ScreenSharing Extension"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": [
        "*://localhost/*",
        "https://*.test.dev/*",
        "https://shishimao.com/*",
        "https://rtcat.io/*",
        "https://ecloudeal.com/*",
        "https://rtc-rooms.com/*",
        "https://v.sishuedu.com/*",
        "https://*.linctime.com/*",
        "https://*.baijiayun.com/*"
      ],
      "js": [
        "inject.js"
      ]
    }
  ],
  "externally_connectable": {
    "matches": [
      "*://localhost/*",
      "https://*.test.dev/*",
      "https://shishimao.com/*",
      "https://rtcat.io/*",
      "https://ecloudeal.com/*",
      "https://rtc-rooms.com/*",
      "https://v.sishuedu.com/*",
      "https://*.linctime.com/*"
    ]
  },
  "permissions": [
    "desktopCapture",
    "*://localhost/*",
    "https://*.linctime.com/*",
    "https://*.baijiayun.com/*"
  ]
}
